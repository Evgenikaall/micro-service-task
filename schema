@startuml

[*] --> READY

READY --> FORK : RUN

FORK --> TASK

TASK --> LISTENED : LISTEN

LISTENED --> CONVERTED : CONVERT

CONVERTED --> SAVED : SAVE

SAVED --> LISTENED : LISTEN

SAVED --> END_LISTENER

END_LISTENER --> JOIN : JOIN

TASK --> GENERATED : GENERATE

GENERATED --> SEND : SEND

SEND --> GENERATED : GENERATE

SEND --> END_GENERATOR

END_GENERATOR --> JOIN : JOIN


JOIN --> READY : REPEAT

@enduml